        <!-- Top content -->
        <div class="top-content">
        	
            <div class="inner-bg">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-6 col-sm-offset-3 form-box">
                        	<div>
                        	<h1>Login to OPTIMA</h1>
          				<p><h3><font size="2" color="gray">Please enter your OPTIMA Username and PIN below</font></h3></p>
                        	<hr />
                         </div>
                            <div class="form-bottom">
			                    <form role="form" action="MemberSubscriptionOTP" method="post" class="login-form">
			                    	<div class="form-group">
			                    		<label class="sr-only" for="form-username">Username</label>
			                        	<input type="text" name="form-username" placeholder="Username" value="#[flowVars.username]" class="form-username form-control" id="form-username" readonly>
			                        </div>
			                        <div class="form-group">
			                        	<label class="sr-only" for="form-password">PIN</label>
			                        	<input type="password" name="form-password" placeholder="PIN" class="form-password form-control" id="form-password">
			                        	<input type="hidden" name="form-ticketid" id="form-ticketid" value="#[flowVars.ticketID]">
			                        </div>
			                        <button type="submit" class="btn">Submit</button>
			                    </form>
		                    </div>
                        </div>
                    </div>
                    <div class="row">
                        	<h3><font size="2" color="gray">Copyright (c) 2018 Jatelindo Perkasa Abadi</font></h3>
