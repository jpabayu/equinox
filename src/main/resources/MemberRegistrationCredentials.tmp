  <body>
    <div class="top-content">
	  <div class="header-icon">
		<a href="https://fello.id/">
			<img src="https://fello.id/assets/img/logo/logo-fello.png" class="logo-fello">
		</a>
	  </div>
		
      <div class="inner-bg">
        <div class="container">
          <div class="row">
            <div class="col-sm-6 col-sm-offset-3 form-box">
              <div>
                <h1 style="margin:0; mso-line-height-rule:exactly;">FELLO Activation</h1>
                <p><h3 style="margin:0; mso-line-height-rule:exactly;"><font size="2" color="gray">Create your PIN below</font></h3></p>
              <hr />
            </div>
            <div class="form-bottom">
              <form role="form" action="#[flowVars.parentUrl]/Credential" method="post" class="login-form">
                <div class="form-group">
                	  <center><span style="border:none; box-shadow: none; font-size: 12; color:red;">#[flowVars.statusResponse]</span></center>
                  <label class="sr-only" for="form-pin">PIN</label>
                  <input type="password" name="form-pin" placeholder="PIN" class="form-pin form-control" id="form-pin" pattern="[0-9]*" size="6" inputmode="numeric">
                </div>
                <div class="form-group">
                  <label class="sr-only" for="form-pin2">Confirm PIN</label>
                  <input type="password" name="form-pin2" placeholder="Confirm PIN" class="form-pin2 form-control" id="form-pin2" pattern="[0-9]*" size="6" inputmode="numeric">
